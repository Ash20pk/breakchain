{
  "name": "breakchain",
  "description": "A Chrome Dino game clone built with Phaser 3 which will post it's data on-chain",
  "version": "1.2.1",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "scripts": {
    "start": "NODE_OPTIONS='--openssl-legacy-provider' webpack serve --config webpack.config.js",
    "build": "NODE_OPTIONS='--openssl-legacy-provider' webpack --config webpack.config.js --env production",
    "lint": "run-s -c lint:js lint:styles",
    "lint:js": "eslint src/**/*.js",
    "lint:styles": "stylelint src/**/*.css",
    "lint:staged": "lint-staged --relative",
    "cm": "cz",
    "prepare": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks"
  },
  "dependencies": {
    "@reown/appkit": "^1.6.9",
    "@reown/appkit-adapter-wagmi": "^1.6.9",
    "@reown/walletkit": "^1.2.1",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-query": "^5.67.2",
    "@wagmi/connectors": "^5.7.9",
    "@wagmi/core": "^2.16.5",
    "@walletconnect/core": "^2.19.0",
    "@walletconnect/utils": "^2.19.0",
    "connectkit": "^1.8.2",
    "phaser": "^3.53.0",
    "viem": "^2.23.9",
    "wagmi": "^2.14.13"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^5.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-webpack": "^7.0.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.5.2",
    "favicons": "^6.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.0",
    "husky": "^5.1.3",
    "ignore": "^5.1.8",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "shx": "^0.3.3",
    "stream-browserify": "^3.0.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "util": "^0.12.5",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1",
    "webpackbar": "^5.0.0-3",
    "workbox-webpack-plugin": "^6.1.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
