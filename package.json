{
  "name": "breakchain",
  "description": "A Chrome Dino game clone built with Phaser 3 which will post it's data on-chain",
  "version": "1.2.1",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "run-s -c lint:js lint:styles",
    "lint:js": "eslint src/**/*.js",
    "lint:styles": "stylelint src/**/*.css",
    "lint:staged": "lint-staged --relative",
    "cm": "cz",
    "prepare": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks"
  },
  "dependencies": {
    "@reown/appkit": "^1.6.9",
    "@reown/appkit-adapter-wagmi": "^1.6.9",
    "@reown/walletkit": "^1.2.1",
    "@tabler/icons-react": "^3.31.0",
    "@tanstack/react-query": "^5.67.2",
    "@wagmi/connectors": "^5.7.9",
    "@wagmi/core": "^2.16.5",
    "@walletconnect/core": "^2.19.0",
    "@walletconnect/utils": "^2.19.0",
    "connectkit": "^1.8.2",
    "phaser": "^3.53.0",
    "viem": "^2.23.9",
    "wagmi": "^2.14.13"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@vitejs/plugin-legacy": "^5.4.3",
    "autoprefixer": "^10.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.1.3",
    "ignore": "^5.1.8",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^2.2.1",
    "shx": "^0.3.3",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "terser": "^5.24.0",
    "vite": "^5.4.14",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.17.5",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
